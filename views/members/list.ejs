<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="../styles.css">
    <link rel="stylesheet" href="styles.css">
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <script src="list.js"></script>
  </head>
  <body onload='initListView(<%= JSON.stringify(members) %>)'>
    <!--HEADER-->
    <div id="nav-container"></div>
    <!--HEADER-->
    <div class="state-dropdown-container">
      <button class="state-dropdown-button">Filter by state</button>
      <div class="state-dropdown-content">
        <% for(var key in states){ %>
          <option onclick='stateFilterFunction(<%=JSON.stringify(members)%>,"<%=key%>")'> <%= JSON.stringify(states[key]).replace(/\"/g, ""); %> </option>
        <% } %>
      </div>
  </div>
  <div id="members-containers">
  <div id="left-member-container">
  </div>
  <div id="right-member-container">
  </div>
</div>
</html>
