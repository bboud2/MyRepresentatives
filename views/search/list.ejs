<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="styles.css">
  </head>
  <a href="/">Home</a>
  <a href="/members/senate">Senate Members</a>
  <a href="/members/house">House Members</a>
  <form action="/search/?input=input_value">
    <input type="text" name="input" ><br>
    <input type="submit" value="Submit">
  </form>
  <body>
      <% for(var i = 0; i < members.length; i++){ %>
        <div class="members">
          <span><%= members[i].short_title + " " + members[i].first_name + " " + members[i].last_name + ": " + members[i].state  %></span>
        </div>
      <% } %>
  </body>
</html>
